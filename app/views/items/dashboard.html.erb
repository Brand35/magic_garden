<h1 class="mt-5 mb-3"> Objets mis en location</h1>
<div class="cards-dashboard">
  <% @items.each do |item| %>
    <div class="card-product-dashboard">
        <%= cl_image_tag item.photo.key %>
        <div class="card-product-infos-dashboard">
        <h2><%= item.name %></h2>
      </div>
    </div>
  <% end %>
</div>
<div class="mt-3 mb-5">
  <%= link_to 'Ajouter un objet', new_item_path, class:'btn btn-primary' %>
</div>
