<div class="cards">
  <% @items.each do |item| %>
    <div class="card-product">
        <%= cl_image_tag item.photo.key %>
        <div class="card-product-infos">
        <h2><%= item.name %></h2>
        <span><%= item.description %></span>
        <span><%= item.available %></span>
      </div>
    </div>
  <% end %>
</div>
