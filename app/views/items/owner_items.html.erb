<h1>hello</h1>
<%=  %>
<div class="container">
  <div class="cards">
    <% @items.each do |item| %>
      <div class="card-product">
        <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" />
        <div class="card-product-infos">
          <h2><%= item.name %></h2>
          <span><%= item.description %></span>
          <span><%= item.available %></span>
        </div>
      </div>
    <% end %>
  </div>
</div>
